    $(this).find(".logo_table1_div_img").css("border", "1px solid #3a59f0").children("img").css("margin-top", "19px");
    $(this).find(".logo_table1_text").addClass("select");
  })

  $("body").on("mouseout", ".logo_table1_div", function () {
    $(this).find(".logo_table1_div_img").css("border", "none").children("img").css("margin-top", "20px");
    $(this).find(".logo_table1_text").removeClass("select");
  })

