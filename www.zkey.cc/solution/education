<script>

  $(".img_2").hide();

  $(".bottom_information_area").mouseover(function () {
    $(this).addClass("bottom_information_area_color");
    $(this).siblings().removeClass("bottom_information_area_color");
    $(this).find(".img_1").hide();
    $(this).find(".img_2").show();
  })

  $(".bottom_information_area").mouseout(function () {
    $(this).removeClass("bottom_information_area_color");
    $(this).siblings().removeClass("bottom_information_area_color");
    $(this).find(".img_2").hide();
    $(this).find(".img_1").show();
  })
</script>
